{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "loglevel": "warning",
    "error": "/var/log/v2ray/error.log"
  },
  "inbounds": [
    {
      "port": V2RAY_PORT,
      "listen": "127.0.0.1",
      "tag": "vmess-in",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "V2RAY_UUID",
            "alterId": 64
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "/V2RAY_PATH"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "outboundTag": "blocked",
        "type": "field",
        "ip": [
          "geoip:private"
        ]
      }
    ]
  }
}